{
  "name": "haystacks",
  "version": "0.0.6",
  "description": "A framework to build any number or any kind of native application or automation solution.",
  "main": "./src/main.js",
  "bin": "./bin/main.js",
  "type": "module",
  "scripts": {
    "build": "cp src bin",
    "deploy": "npm run build && node ./build/buildRelease/src/buildRelease.js",
    "release": "babel-node ./build/buildRelease/src/buildRelease.js",
    "test": "node ./test/testHarness/src/testHarness.js",
    "start": "node ./src/main.js",
    "vers": "standard-version -m \"chore(release) version %s \"",
    "docs": "jsdoc -c jsdoc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SethEden/haystacks.git"
  },
  "keywords": [
    "application",
    "automation"
  ],
  "author": "Seth Hollingsead <iceversaka@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "5.0.1",
    "dotenv": "^16.0.0",
    "figlet": "^1.5.2",
    "haystacks": "^0.0.1",
    "mathjs": "10.1.0",
    "moment": "2.29.1",
    "n-readlines": "^1.0.1",
    "papaparse": "5.3.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jsdoc": "3.6.7",
    "prettier": "^2.6.0",
    "standard-version": "9.3.2"
  },
  "bugs": {
    "url": "https://github.com/SethEden/haystacks/issues"
  },
  "homepage": "https://github.com/SethEden/haystacks#readme",
  "volta": {
    "node": "16.14.2"
  }
}
